# Google Sign-In Configuration
# Copy this file to .env and fill in your actual credentials from Google Cloud Console

# Web Client ID (for web platform)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_web_client_id.apps.googleusercontent.com

# iOS Client ID (for iOS platform)
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_ios_client_id.apps.googleusercontent.com

# Android Client ID (for Android platform)
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your_android_client_id.apps.googleusercontent.com

# Client Secret (for server-side token exchange)
EXPO_PUBLIC_GOOGLE_CLIENT_SECRET=your_client_secret_here

# Firebase Configuration (if not already configured)
# EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
# EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
# EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
# EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
# EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
# EXPO_PUBLIC_FIREBASE_APP_ID=your_app_id
